# Autobuild Manifests
if [ "x$ABMK" = "x" ]; then
    ABMK="-j$(echo $(lscpu | grep ^CPU\(s\) | awk '{ print $2 }')*2+1 | bc)"
else
    true
fi

ABARCHIVE=autobuild-aoscarchive
ABINSTALL=yes
ABSHADOW=yes

# LTO section
NOLTO=yes

# Clang or not?
USECLANG=no

# Want those big fat static libraries?
NOSTATIC=yes
