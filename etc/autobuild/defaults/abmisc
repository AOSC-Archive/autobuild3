# Autobuild Manifests
if [ "x$ABMK" = "x" ]; then
    ABMK="-j$(echo $(lscpu | grep ^CPU\(s\) | awk '{ print $2 }')*2+1 | bc) CFLAGS=${CFLAGS} CPPFLAGS=${CPPFLAGS} CXXFLAGS=${CXXFLAGS} LDFLAGS=${LDFLAGS} V=1"
else
    ABMK+=" CFLAGS=${CFLAGS} CPPFLAGS=${CPPFLAGS} CXXFLAGS=${CXXFLAGS} LDFLAGS=${LDFLAGS}"
fi
ABARCHIVE=autobuild-aoscarchive
ABINSTALL=yes
ABSHADOW=yes

# LTO section
NOLTO=yes

# Clang or not?
USECLANG=no
